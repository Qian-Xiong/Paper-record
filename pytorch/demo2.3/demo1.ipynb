    "import torch\n",
    "x=torch.tensor([[1.0,2.0],[3.0,4.0]],requires_grad=True)\n",
    "##默认requires_grad=False\n",
    "y=torch.sum(x**2+2*x+1)\n",
    "print(\"x.requires_grad:\",x.requires_grad)\n",
    "print(\"y.requires_grad:\",y.requires_grad)\n",
    "print(\"x:\",x)\n",
    "print(\"y:\",y)"
